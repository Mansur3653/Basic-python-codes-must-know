n = int(input("Value: "))
a, b = 0, 1
for i in range(n):
    print(a)
    c = a+b
    a = b
    b = c

#OR

n = int(input("Value: "))
a, b = 0, 1
for i in range(n):
    if a<n:
        print(a)
    c = a+b
    a = b
    b = c

#OR

n = int(input("Value: "))
a, b = 0, 1
while(a<n):
    print(a)
    c = a+b
    a = b
    b = c
